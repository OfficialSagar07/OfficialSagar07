#include<stdio.h>
 int main (){
 char ch;
 printf("Enter any charecter :");
 scanf("%c",&ch);
  if (ch>='A' && ch<='Z'){
      printf("The charecter is upper case");
  }else if ( ch>='a' && ch<= 'z'){
      printf("The charecter is lower case");
  }else{
      printf("niether the upper case nor a lower case");
      
  }
  return 0;
  }
